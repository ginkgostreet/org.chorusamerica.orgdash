<?xml version="1.0"?>
<extension key="org.chorusamerica.orgdash" type="module">
  <file>orgdash</file>
  <name>Organization Dashboard</name>
  <description>Provides an interface for appropriately permissioned users to administer the organizations to which they belong</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Chorus America</author>
    <email>webmaster@chorusamerica.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/ginkgostreet/org.chorusamerica.orgdash</url>
    <url desc="Documentation">https://github.com/ginkgostreet/org.chorusamerica.orgdash#readme</url>
    <url desc="Support">https://github.com/ginkgostreet/org.chorusamerica.orgdash/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2019-01-06</releaseDate>
  <version>0.1.0</version>
  <compatibility>
    <ver>5.10</ver>
  </compatibility>
  <requires>
    <ext>nz.co.fuzion.relatedpermissions</ext>
    <ext>org.civicrm.fieldmetadata</ext>
  </requires>
  <comments>
    Configuration at Administer > System Settings > Organization Dashboard.
    Dashboard appears at CiviCRM path /civicrm/orgdash/#/[organization_contact_id].
  </comments>
  <civix>
    <namespace>CRM/Orgdash</namespace>
  </civix>
</extension>
